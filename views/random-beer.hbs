<div>    
        {{#each responseFromAPI}}
           <img class="random-beer-picture" height="200px" src={{image_url}} alt="">
           <h2>{{name}}</h2>
           <p>description : {{description}}</p>
           <p>tagline : {{tagline}}</p>
           <h2>Food Pairing:</h2>
            {{#each food_pairing}}
                <li>{{this}}</li>
            {{/each}}
        {{/each}}
           {{!-- <p>food_pairing: {{food_pairing}}</p> --}}
        <p>brewers_tips: {{brewers_tips}}</p>

 </div>